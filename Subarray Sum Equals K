class Solution {
    public int subarraySum(int[] nums, int k) {
        int sum =0;
        int count = 0;
        HashMap<Integer,Integer>hs = new HashMap<>();
        hs.put(0, 1);
        for(int i : nums){
            sum += i;
            if(hs.containsKey(sum-k)){
                count += hs.get(sum-k);
            }
            hs.put(sum,hs.getOrDefault(sum,0)+1);
        }
        return count;
    }
}
